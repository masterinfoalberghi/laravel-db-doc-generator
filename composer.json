{
    "name": "masterinfoalberghi/laravel-db-doc-generator",
    "type": "library",
    "description": "Database documentation generator for laravel",
    "keywords": [
        "laravel",
        "database documentation",
        "laravel documentation"
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Churakov Mike"
        }
    ],
    "require": {
        "ext-tokenizer": "*",
        "doctrine/dbal": "*",
        "illuminate/console": "*",
        "illuminate/contracts": "*",
        "illuminate/database": "*",
        "illuminate/http": "*",
        "illuminate/pagination": "*",
        "illuminate/support": "*",
        "illuminate/view": "*"
    },
    "autoload": {
        "psr-4": {
            "ChurakovMike\\DbDocumentor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChurakovMike\\DbDocumentor\\DbDocumentorServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
